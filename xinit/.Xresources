Xft.dpi:   100
! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6


! Black color that will not be affected by bold highlighting.
*.color66: #09050a
*color66:  #09050a

! Xclock colors.
XClock*foreground: #e3d3c9
XClock*background: #09050a
XClock*majorColor:  rgba:e3/d3/c9/ff
XClock*minorColor:  rgba:e3/d3/c9/ff
XClock*hourColor:   rgba:e3/d3/c9/ff
XClock*minuteColor: rgba:e3/d3/c9/ff
XClock*secondColor: rgba:e3/d3/c9/ff

! Set depth to make transparency work.
URxvt*depth: 32
Xcursor.size: 16
